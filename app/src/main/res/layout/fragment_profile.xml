<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#FFFFFFFF"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user.fragment.ProfileFragment">
    <ScrollView
        android:fillViewport="true"
        android:id="@+id/r57p3p049ict"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FFFFFFFF"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/r92jssnavuwd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFFFF"
                android:paddingVertical="20dp"
                android:paddingHorizontal="22dp"
                android:layout_marginBottom="30dp"
                android:elevation="2dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r0fd4sh3oua9l"
                    android:textColor="#000000"
                    android:textSize="22sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Profile"
                    />
            </LinearLayout>
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/iv_profile"

                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginBottom="19dp"
                android:layout_gravity="center"
                android:src="@drawable/error_image"
                android:scaleType="fitXY"
                />
            <TextView
                android:id="@+id/tv_username"
                android:textColor="#3E3E3E"
                android:textSize="21sp"
                android:layout_marginBottom="9dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Joe Ordinary"
                />
            <TextView
                android:id="@+id/tv_email"
                android:textColor="#6B6B6B"
                android:textSize="14sp"
                android:layout_marginBottom="38dp"

                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="example@gmail.com"
                />
            <LinearLayout
                android:id="@+id/r8zv1uhw0j5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:paddingVertical="12dp"
                android:paddingHorizontal="14dp"
                android:layout_marginBottom="17dp"
                android:layout_marginHorizontal="15dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rgtri6am569t"
                    android:textColor="#000000"
                    android:textSize="17sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="My Profile"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/recni03u116"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="19dp"
                android:layout_marginHorizontal="31dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rl7usko3wvrn"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_marginEnd="21dp"
                    android:src="@drawable/ic_cart"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/tv_orders"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Orders"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/reyblly5cxf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="18dp"
                android:layout_marginHorizontal="31dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rvk94znlrmv"
                    android:layout_width="16dp"
                    android:layout_height="12dp"
                    android:layout_marginEnd="22dp"
                    android:src="@drawable/ic_card"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/tv_billing"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Billing"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/signout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="28dp"
                android:layout_marginHorizontal="31dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/signout_image"
                    android:layout_width="16dp"
                    android:layout_height="16dp"
                    android:layout_marginEnd="21dp"
                    android:src="@drawable/ic_logout"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/tv_signout"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Sign out"
                    />
            </LinearLayout>




        </LinearLayout>
    </ScrollView>
</FrameLayout>
